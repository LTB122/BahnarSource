{
  "name": "bahnar",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro Max'",
    "ipad": "react-native run-ios --simulator='iPad Pro (11-inch) (4th generation) '",
    "devtools": "react-devtools",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --fix --max-warnings=0 --cache-location './node_modules/.cache/.eslintcache'",
    "format": "prettier . --write",
    "bundle:android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "bundle:ios": "npx react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/Bahnar/main.jsbundle --assets-dest ios",
    "build:android": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "prepare": "husky install",
    "pod": "cd ios && pod repo update && pod install && cd ../",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "watchman:clean": "watchman watch-del-all",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "axios": "^1.4.0",
    "deprecated-react-native-prop-types": "^4.2.1",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-config": "^1.5.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-event-listeners": "^1.0.7",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-image-picker": "^5.6.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-pager-view": "^6.2.0",
    "react-native-reanimated": "^3.4.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.22.1",
    "react-native-svg": "^13.10.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-track-player": "^3.2.0",
    "react-native-vision-camera": "^2.15.5",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/lodash": "^4.14.197",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-reactnative": "^1.0.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "husky": ">=6",
    "jest": "^29.2.1",
    "lint-staged": ">=10",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --max-warnings=0 --fix",
    "*.{js,ts,tsx,md,json}": "prettier --write"
  }
}
